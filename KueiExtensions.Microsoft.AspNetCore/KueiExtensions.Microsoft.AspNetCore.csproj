    <Authors>Kuei</Authors>
    <Company>RagnaKuei</Company>
    <PackageProjectUrl>https://github.com/ragnakuei/KueiExtensions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ragnakuei/KueiExtensions</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <LangVersion>latest</LangVersion>
    <PackageVersion>1.0.17</PackageVersion>
    <OutputType>Library</OutputType>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\KueiExtensions\KueiExtensions.csproj" />
  </ItemGroup>

</Project>
